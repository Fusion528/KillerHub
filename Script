-- Scripts:

local function KURFO_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	script.Parent.MouseButton1Down:Connect(function()
		local keys = {
			"212",
			"213"
		}
	
	
		if script.Parent.Parent.Parent.TextBox.Text == "fusionwinning" then
			script.Parent.Parent.TextButton.Text = "Reading Key.."
			wait(2)
			script.Parent.Parent.TextButton.Text = "Authenticating User...."
			wait(2)
			script.Parent.Parent.TextButton.Text = "Granted. Have fun!"
			wait(3)
			script.Parent.Parent.Parent.Parent.MainUI.Visible = true
			wait(0)
			script.Parent.Parent.Parent.Parent.Frame.Visible = false
		else
			script.Parent.Parent.TextButton.Text = "Reading Key.."
			wait(2)
			script.Parent.Parent.TextButton.Text = "Key is invalid!"
			wait(4)
			script.Parent.Parent.TextButton.Text = "Submit Key"
		end
	
	end)
end
coroutine.wrap(KURFO_fake_script)()
local function HORGLVN_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame)

	local UserInputService = game:GetService("UserInputService")
	
	local poopoo = script.Parent
	
	local noob
	local baba
	local babya
	local yayaya
	
	local function update(input)
		local delta = input.Position - babya
		poopoo.Position = poopoo:TweenPosition(UDim2.new(yayaya.X.Scale, yayaya.X.Offset + delta.X, yayaya.Y.Scale, yayaya.Y.Offset + delta.Y), 'Out', 'Quart', 4, true); -- drag speed
	end
	
	poopoo.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			noob = true
			babya = input.Position
			yayaya = poopoo.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					noob = false
				end
			end)
		end
	end)
	
	poopoo.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			baba = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == baba and noob then
			update(input)
		end
	end)
end
coroutine.wrap(HORGLVN_fake_script)()
local function CGRHZK_fake_script() -- TurnOffMags.Script 
	local script = Instance.new('Script', TurnOffMags)

	script.Parent.MouseButton1Down:Connect(function()
		local Enabled = false
	
		if Enabled == true then
	
			game.StarterGui:SetCore("SendNotification", {
				Title = "Backtracked#0590";
				Text = "Mags Loaded (R to mag)";
				Icon = "rbxassetid://ExampleIcon";
				Duration = 7;
			})
		end
		script.Parent.Parent.TurnOffMags.Visible = false
		script.Parent.Parent.TurnOnMags.Visible = true
		
		game.StarterGui:SetCore("SendNotification", {
			Title = "Backtracked#0590";
			Text = "Mags Unloaded";
			Icon = "rbxassetid://ExampleIcon";
			Duration = 3;
		})
	end)
end
coroutine.wrap(CGRHZK_fake_script)()
local function WBJKQ_fake_script() -- TurnOnMags.Script 
	local script = Instance.new('Script', TurnOnMags)

	script.Parent.MouseButton1Down:Connect(function()
		local Enabled = true
	
		if Enabled == true then
			game.Players.LocalPlayer:GetMouse().KeyDown:connect(function(key)
				if key == "r" then 
					for i,v in pairs(workspace:GetChildren())do
						if v:IsA("Part") and v.Name == "Football"then
							wait()
							workspace.Football.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
						end
					end
				end
			end)
	
			game.StarterGui:SetCore("SendNotification", {
				Title = "Backtracked#0590";
				Text = "Mags Loaded (R to mag)";
				Icon = "rbxassetid://ExampleIcon";
				Duration = 7;
			})
		end
		script.Parent.Parent.TurnOffMags.Visible = true
		script.Parent.Parent.TurnOnMags.Visible = false
	end)
end
coroutine.wrap(WBJKQ_fake_script)()
local function ABYZ_fake_script() -- TurnOnBallTP.Script 
	local script = Instance.new('Script', TurnOnBallTP)

	script.Parent.MouseButton1Down:Connect(function()
	
		getgenv().Hype = true; -- Change this to false to disable it!
	
		if getgenv().Hype == true then
			game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
				if key == "z" then
					game.Players.LocalPlayer.Character.HumanoidRootPart.Position = game.Workspace.Football.Position
				end	
			end)
		end
		script.Parent.Parent.TurnOnBallTP.Visible = false
		wait(0)
		script.Parent.Parent.TurnOffBallTP.Visible = true
		
		game.StarterGui:SetCore("SendNotification", {
			Title = "Backtracked#0590";
			Text = "TP to ball loaded (Z to TP)";
			Icon = "rbxassetid://ExampleIcon";
			Duration = 7;
		})
	end)
end
coroutine.wrap(ABYZ_fake_script)()
local function HITYXZX_fake_script() -- TurnOffBallTP.Script 
	local script = Instance.new('Script', TurnOffBallTP)

	script.Parent.MouseButton1Down:Connect(function()
	
		getgenv().Hype = false; -- Change this to false to disable it!
	
		if getgenv().Hype == true then
			game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
				if key == "z" then
					game.Players.LocalPlayer.Character.HumanoidRootPart.Position = game.Workspace.Football.CFrame
				end	
			end)
		end
		script.Parent.Parent.TurnOnBallTP.Visible = true
		wait(0)
		script.Parent.Parent.TurnOffBallTP.Visible = false
	end)
end
coroutine.wrap(HITYXZX_fake_script)()
local function LWWW_fake_script() -- Mossed.Script 
	local script = Instance.new('Script', Mossed)

	local UserInputService = game:GetService("UserInputService")
	
	local poopoo = script.Parent
	
	local noob
	local baba
	local babya
	local yayaya
	
	local function update(input)
		local delta = input.Position - babya
		poopoo.Position = poopoo:TweenPosition(UDim2.new(yayaya.X.Scale, yayaya.X.Offset + delta.X, yayaya.Y.Scale, yayaya.Y.Offset + delta.Y), 'Out', 'Quart', 4, true); -- drag speed
	end
	
	poopoo.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			noob = true
			babya = input.Position
			yayaya = poopoo.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					noob = false
				end
			end)
		end
	end)
	
	poopoo.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			baba = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == baba and noob then
			update(input)
		end
	end)
end
