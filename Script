-- Scripts:

local function CLFFU_fake_script() -- Execute.Script 
	local script = Instance.new('Script', Execute)

	script.Parent.MouseButton1Down:Connect(function()
		loadstring(script.Parent.Parent.Parent.ScriptBox.Text)()
	end)
end
coroutine.wrap(CLFFU_fake_script)()
local function EFGZC_fake_script() -- Clear.Script 
	local script = Instance.new('Script', Clear)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent.ScriptBox.Text = "Text Cleared!"
		wait(2)
		script.Parent.Parent.Parent.ScriptBox.Text = ""
	end)
end
coroutine.wrap(EFGZC_fake_script)()
local function TJLET_fake_script() -- Attach.Script 
	local script = Instance.new('Script', Attach)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Text = "Attaching."
		wait(0.5)
		script.Parent.Text = "Attaching.."
		wait(1)
		script.Parent.Text = "Ready!"
		script.Parent.Parent.Parent.ScriptBox.TextEditable = true
		wait(1)
		script.Parent.Text = "Attached"
	end)
end
coroutine.wrap(TJLET_fake_script)()
local function XPBYCJU_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(XPBYCJU_fake_script)()
local function XHQX_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame)

	script.Parent.ScriptBox.TextEditable = false
end
coroutine.wrap(XHQX_fake_script)()
