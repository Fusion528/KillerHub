local function GTELOI_fake_script() -- Mossed_2.Script 
	local script = Instance.new('Script', Mossed_2)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent.Parent.MainUI.Visible = false
		wait(0)
		script.Parent.Parent.Parent.Parent.Mossed.Visible = true
	end)
end
coroutine.wrap(GTELOI_fake_script)()
